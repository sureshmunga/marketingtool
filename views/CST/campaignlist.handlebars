<form id="frmcampaignlist">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="javascript:void(0)">CST</a></li>
        <li class="breadcrumb-item active">Campaign List</li>
    </ol>

    <div class="row">
        
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="form-group">
                <a id="btnCreateNew" href="/cst/campaign" class="btn btn-primary align-left">
                    Create New
                </a>
            </div>
        </div>
    </div>

    <table class="table table-striped">
        <thead>
            <tr>
                <th>
                    Campaign Id
                </th>
                <th>campaign</th>
                <th>Manager's email</th>
                {{!-- <th>Description</th> --}}
                <th>Start Date</th>
                <th>End Date</th>
                <th>Status</th>
                <th>Creator</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            {{#each campaignlist}}
            <tr>
                <td>{{campaignid}}</td>
                <td>{{campaignname}}</td>
                <td>{{manageremail}}</td>
                {{!-- <td>{{Description}}</td> --}}
                <td>{{startdate}}</td>
                <td>{{enddatae}}</td>
                <td>{{status}}</td>
                <td>{{createdby}}</td>
                <td>
                    <a href="/cst/campaign/{{id}}" title="Edit" campaignId={{campaignid}} value="Edit"><i class="fas fa-edit"></i></a>
                    <a href="#" title="Delete" campaignId={{campaignid}} value="Delete">
                        <i class="fas fa-trash"></i>
                    </a>
                    {{!-- <a href="#" title="Hide" campaignId={{campaignid}} value="Hide">
                        <i class="fas fa-eye-slash"></i>
                    </a> --}}
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</form>