        <!-- Page Content start Holder -->
        <section class="h-100">
            <div class="row justify-content-md-center h-100">
                <div class="card-wrapper">
                    <div class="card fat">
                        <div class="card-body">

                            <div id="content">
                                <nav class="navbar navbar-default">
                                    <div class="container-fluid">
                                        <h2> Tactic </h2>
                                        <div class="navbar-header">
                                        </div>
                                        <div class="panel panel-default">
                                            <div class="panel-body">
                                                <form method="post" action="/tactic/tacticsave">
                                                    <input type="hidden" id="tacticid" value={{tactic.tacticid}}>
                                                    <!--Select Campaign here-->
                                                    <div class="form-group required-field">
                                                        <label for="CN">Campaign {{tactic.mastercampaignid}}</label>
                                                        <hidden id="span" value={{tactic.mastercampaignid}}></hidden>
                                                        <select class="form-control" id="drpCampaign" name="campaign" autofocus>
                                                            <option value="0">--Select Campaign--</option>
                                                            {{#each campaign}} {{!--
                                                            <option {{if this tactic.mastercampaignid}} value={{mastercampaignid}}>{{mastercampaignname}}</option> --}} {{#if isselect}}
                                                            <option selected="selected" value={{mastercampaignid}}>{{mastercampaignname}}</option>
                                                            {{else}}
                                                            <option value={{mastercampaignid}}>{{mastercampaignname}}</option>
                                                            {{/if}} {{/each}}

                                                        </select>
                                                        </br>

                                                    </div>
                                                    <div class="clear"></div>

                                                    <!--Select Program here-->
                                                    <div class="form-group required-field">
                                                        <label for="PG">Program</label>
                                                        <select class="form-control" id="drpProgram" name="program" autofocus>
                                                            <option value="0">--Select Program--</option>
                                                            {{#each programfamilies}} {{#if isselect}}
                                                            <option selected="selected" value={{programid}}>{{programname}}</option>
                                                            {{else}}
                                                            <option value={{programid}}>{{programname}}</option>
                                                            {{/if}} {{/each}} </select>
                                                        </br>
                                                    </div>
                                                    <div class="clear"></div>

                                                    <!--Select Pgogram JOB here-->
                                                    <div class="form-group required-field">
                                                        <label for="PN">Program Name</label>
                                                        <input id="proName" type="text" class="form-control" name="programname" value="" required autofocus>
                                                        </br>
                                                    </div>
                                                    <div class="clear"></div>

                                                    <!--Select Tactic Type here-->
                                                    <div class="form-group required-field">
                                                        <label for="PM">Campaign Manager</label>
                                                        <input id="campaignmanager" type="text" class="form-control" name="campaignmanager" value="" required autofocus>
                                                        </br>
                                                    </div>

                                                    <!--Enter Tactic Name here-->
                                                    <div class="form-group">
                                                        <label for="PDG">Program Desciption & Goals</label>
                                                        <input id="prodecgoals" type="text" class="form-control" name="prodescgoals" value="" required autofocus>

                                                    </div>

                                                    <!--Select Markets here-->
                                                    <div class="form-group required-field">
                                                        <label for="MAR">Markets</label>
                                                        <select class="chosen-select  form-control" id="lstMarkets" name="Markets" multiple="true">
                                                            {{#each market}}
                                                            <option {{#if isselect}} selected="selected" {{/if}} value={{marketid}}>{{marketname}}</option>
                                                            {{/each}} </select>
                                                        </br>
                                                    </div>

                                                    <!--Enter Start Date here-->
                                                    <div class="form-group required-field">
                                                        <label for="SD">Start Date</label>
                                                        <input id="txtStartDate" type="date" class="form-control" name="StartDate" value={{tactic.startdate}} autofocus>

                                                    </div>

                                                    <!--Enter End Date here-->
                                                    <div class="form-group required-field">
                                                        <label for="ED">End Date</label>
                                                        <input id="txtEndDate" type="date" class="form-control" name="EndDate" value={{tactic.enddate}} autofocus>

                                                    </div>

                                                    <!--Select MCA Segment here-->
                                                    <div class="form-group required-field">
                                                        <label for="MS">MCA Segment</label>
                                                        <select class="form-control" id="drpMCASegment" name="MCASegment" autofocus>
                                                            <option value="0">--Select MCA Segment--</option>
                                                            {{#each mcasegment}} {{#if isselect}}
                                                            <option selected="selected" value={{mcasegmentid}}>{{mcasegmentname}}</option>
                                                            {{else}}
                                                            <option value={{mcasegmentid}}>{{mcasegmentname}}</option>
                                                            {{/if}} {{/each}} </select>
                                                        </br>
                                                    </div>




                                                    <!--Select Business Group here-->
                                                    <div class="form-group required-field">
                                                        <label for="BG">Lead Business Group</label>
                                                        <select class="form-control" id="drpBusinessGroup" name="BusinessGroup" autofocus>
                                                            <option value="0">--Select Business Group--</option>
                                                            {{#each businessgroups}}
                                                            <option {{#if isselect}} selected="selected" {{/if}} value={{businessgroupid}}>{{businessgroupname}}</option>
                                                            {{/each}} </select>
                                                        </br>
                                                    </div>

                                                    <!--Select Business Line here-->
                                                    <div class="form-group required-field">
                                                        <label for="BL">Lead Business Line</label>
                                                        <select class="form-control" id="drpBusinessLine" name="BusinessLine" autofocus>
                                                            <option value="0">--Select Business Line--</option>
                                                            {{#each businessline}}
                                                            <option {{#if isselect}} selected="selected" {{/if}} value={{businesslineid}}>{{businesslinename}}</option>
                                                            {{/each}} </select>
                                                        </br>
                                                    </div>

                                                    <!--Select sec Business groups here-->
                                                    <div class="form-group required-field">
                                                        <label for="secBusGroup">Secondary Business Group</label>
                                                        <select class="chosen-select  form-control" id="secbusinessgroups" name="secbusinessgroups" multiple="true">
                                                            {{#each businessgroups}}
                                                            <option {{#if isselect}} selected="selected" {{/if}} value={{businessgroupid}}>{{businessgroupname}}</option>
                                                            {{/each}} </select>
                                                        </br>
                                                    </div>
                                                    <!--Select sec Business lines here-->
                                                    <div class="form-group required-field">
                                                        <label for="secBusLine">Secondary Business Lines</label>
                                                        <select class="chosen-select  form-control" id="secbusinessline" name="secbusinessline" multiple="true">
                                                            {{#each businessline}}
                                                            <option {{#if isselect}} selected="selected" {{/if}} value={{businesslineid}}>{{businesslinename}}</option>
                                                            {{/each}} </select>
                                                        </br>
                                                    </div>


                                                    <!--Select lead Business Type here-->
                                                    <div class="form-group required-field">
                                                        <label for="LBT">Lead Business Type</label>
                                                        <select class="form-control" id="leadBusinessType" name="LeadBusinessType" autofocus>
                                                            <option value="0">--Select Business Type--</option>
                                                            {{#each businesstype}}
                                                            <option {{#if isselect}} selected="selected" {{/if}} value={{businesstypeid}}>{{businesstypename}}</option>
                                                            {{/each}} </select>
                                                        </br>
                                                    </div>

                                                    <!--Select industry here-->
                                                    <div class="form-group required-field">
                                                        <label for="ind">Industry</label>
                                                        <select class="form-control" id="indus" name="industry" autofocus>
                                                            <option value="0">--Select Industry--</option>
                                                            {{#each industry}}
                                                            <option {{#if isselect}} selected="selected" {{/if}} value={{industryid}}>{{industryname}}</option>
                                                            {{/each}} </select>
                                                        </br>
                                                    </div>

                                                    <!--Select Business Line here-->
                                                    <div class="form-group required-field">
                                                        <label for="BL">Lead Business Line</label>
                                                        <select class="form-control" id="drpBusinessLine" name="BusinessLine" autofocus>
                                                            <option value="0">--Select Business Line--</option>
                                                            {{#each businessline}}
                                                            <option {{#if isselect}} selected="selected" {{/if}} value={{businesslineid}}>{{businesslinename}}</option>
