                                                        <select class="form-control" id="drpIndustry" name="Industry" autofocus>
                                                            <option value="0">--Select Industry--</option>
                                                            {{#each Industry}}
                                                            <option {{#if isselect}} selected="selected" {{/if}} value={{industryid}}>{{industryname}}</option>
                                                            {{/each}} </select>
                                                        </br>
                                                    </div>

                                                    <!--Select sec Business lines here-->
                                                    <div class="form-group required-field">
                                                        <label for="secBusLine">Secondary Business Type</label>
                                                        <select class="chosen-select  form-control" id="secbusinesstype" name="secbusinesstype" multiple="true">
                                                            {{#each businesstype}}
                                                            <option {{#if isselect}} selected="selected" {{/if}} value={{businesstypeid}}>{{businesstypename}}</option>
                                                            {{/each}} </select>
                                                        </br>
                                                    </div>

                                                    <div class="panel-footer">
                                                        <button id="btnSubmit" type="submit" class="btn btn-primary">Submit</button>
                                                        <button type="button" class="btn btn-default" id="btntacticDraft">Save Draft</button>
                                                    </div>
                                            </div>
                                        </div>
                                </nav>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
