<form id="frmprogramist">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="javascript:void(0)">CST</a></li>
        <li class="breadcrumb-item active">Program List</li>
    </ol>

    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="form-group">
                <a id="btnCreateNew" href="/program/program" class="btn btn-primary align-left">
                    Create New
                </a>
            </div>
        </div>
    </div>
    <table class='table table-striped'>
        <tr>
            <th>
                Program Id
            </th>
            <th>Program</th>
            <th>Manager's email</th>
            {{!-- <th>Description</th> --}}
            <th>Start Date</th>
            <th>End Date</th>
            <th>Status</th>
            <th>Creator</th>
            <th>Action</th>
        </tr>
        {{#each programlist}}
        <tr>
            <td>{{programdigitalid}}</td>
            <td>{{programname}}</td>
            <td>{{campaignmanager}}</td>
            {{!-- <td>{{programdescription}}</td> --}}
            <td>{{startdate}}</td>
            <td>{{enddate}}</td>
            <td>{{status}}</td>
            <td>{{createdby}}</td>
            <td>
                <a href="/program/program/{{programid}}" title="Edit" programid={{programid}} value="Edit">
                    <i class="fas fa-edit"></i></a>
                <a href="#" title="Delete" programid={{programid}} value="Delete">
                    <i class="fas fa-trash"></i>
                </a>
                {{!-- <a href="#" title="Hide" campaignId={{campaignid}} value="Hide">
                    <i class="fas fa-eye-slash"></i>
                </a> --}}

            </td>
        </tr>
        {{/each}}
    </table>
</form>